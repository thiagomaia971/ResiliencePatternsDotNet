@page "/chart-box-plot/compiled/grouped"
@inject ChartBoxValuesService ChartBoxValuesService

<ChartBox generate="Generate" name="Tempo Total das Requisições"></ChartBox>

@code {

    private async Task<Dictionary<string, List<BoxandWhiskerData>>> Generate(
        IFileListEntry[] files, 
        IFileListEntry[] baseLineOne, 
        IFileListEntry[] baseLineTwo, 
        string system)
    {
        return await ChartBoxValuesService.GetClientToModuleTotalTimeByPath(files, baseLineOne, baseLineTwo, system, x => x.ClientToModuleTotalTime);
    }

}