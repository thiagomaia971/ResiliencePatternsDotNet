@page "/chart-box-plot/compiled/grouped/average"
@inject ChartBoxValuesService ChartBoxValuesService

<ChartBox generate="Generate" name="Tempo Médio das Requisições"></ChartBox>

@code {

    private async Task<Dictionary<string, List<BoxandWhiskerData>>> Generate(
        IFileListEntry[] files,
        IFileListEntry[] baseLineOne,
        IFileListEntry[] baseLineTwo,
        string system)
    {
        return await ChartBoxValuesService.GetResilienceModuleToExternalAverageTimePerRequestTimeByPath(files, baseLineOne, baseLineTwo, system, x => x.ClientToModuleTotalTime);
    }

}