@page "/chart-box-plot/compiled/grouped/errors/fracionado"
@inject ChartBoxValuesService ChartBoxValuesService

<ChartBox generate="Generate" name="Tempo total de requisições com erro" nameKey="TotalTimeError"></ChartBox>

@code {

    private async Task<Dictionary<string, List<BoxandWhiskerData>>> Generate(
        IFileListEntry[] files,
        string system)
    {
        return await ChartBoxValuesService.GetAllDatas(files: files,
            system: system,
            groupBy: x => x.ScenarioSucessTax,
            groupByName: x => $"{x.ScenarioGroup} clientes simultâneos",
            selector: x =>
            {
                return x.ResilienceModuleToExternalTotalErrorTime;
            });
    }

}